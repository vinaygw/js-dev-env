{
  "name": "js-dev-env",
  "version": "1.0.0",
  "description": "js dev environment",
  "scripts": {
    "prestart": "babel-node buildScripts/startScript.js",
    "start": "npm-run-all --parallel audit-force open:src lint:watch test:watch ",
    "open:src": "babel-node buildScripts/srcServer.js",
    "audit": "npm audit",
    "audit-force": "npm audit fix --force",
    "share": "lt --port 3030",
    "lint": "esw webpack.config.* src buildScripts --color",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress buildScripts/testSetup.js \"src/**/*.test.js\" ",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Cory House",
  "license": "MIT",
  "dependencies": {
    "whatwg-fetch": "1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "babel-cli": "6.16.0",
    "babel-core": "6.17.0",
    "babel-loader": "7.1.4",
    "babel-preset-latest": "6.16.0",
    "babel-register": "6.16.3",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "cheerio": "0.22.0",
    "compression": "^1.7.4",
    "cross-env": "3.1.3",
    "css-loader": "^4.2.1",
    "eslint": "3.8.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-watch": "^7.0.0",
    "express": "^4.17.1",
    "extract-text-webpack-plugin": "1.0.1",
    "html-webpack-plugin": "^4.3.0",
    "jsdom": "9.8.0",
    "json-schema-faker": "^0.4.7",
    "json-server": "^0.16.1",
    "localtunnel": "^2.0.0",
    "mocha": "^8.1.1",
    "nock": "^13.0.4",
    "npm-run-all": "3.1.1",
    "nsp": "2.6.2",
    "numeral": "1.5.3",
    "open": "^7.1.0",
    "rimraf": "2.5.4",
    "style-loader": "0.13.1",
    "surge": "^0.21.6",
    "webpack": "^4.44.1",
    "webpack-dev-middleware": "1.8.4",
    "webpack-hot-middleware": "2.13.0",
    "webpack-md5-hash": "0.0.5"
  }
}
